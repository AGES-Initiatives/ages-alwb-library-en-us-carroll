peMA.Ode4C1H.ode = misc_en_US_carroll.Ode4
peMA.Ode4C1H.mode = misc_en_US_carroll.Mode1
peMA.Ode4C1H.melody = he.h.m1_en_US_carroll.MegaToMystirion.melody
peMA.Ode4C1H.incipit = he.h.m1_en_US_carroll.MegaToMystirion.incipit
peMA.Ode4C1H.text = he.h.m1_en_US_carroll.MegaToMystirion.text

peMA.Ode4C11.ode = misc_en_US_carroll.Ode4
peMA.Ode4C11.mode = misc_en_US_carroll.Mode1
peMA.Ode4C11.melody = he.h.m1_en_US_carroll.MegaToMystirion.name
peMA.Ode4C11.incipit = "Then He tasted gall…"
peMA.Ode4C11.text = "Then He tasted gall and healed our tasting of the tree of knowledge; * now, as He partakes of the sweetness of honeycomb before His friends, * to Adam He gives in turn * a sweeter grace: to partake of His deity."

peMA.Ode4C12.ode = misc_en_US_carroll.Ode4
peMA.Ode4C12.mode = misc_en_US_carroll.Mode1
peMA.Ode4C12.melody = he.h.m1_en_US_carroll.MegaToMystirion.name
peMA.Ode4C12.incipit = "You delight…"
peMA.Ode4C12.text = "You delight, O Friend of Man, in being studied and examined; * hence You urge your doubting disciple with doubting hand to probe Your side * and prove thus to all the world * that You arose in the flesh indisputably."

peMA.Ode4C13.ode = misc_en_US_carroll.Ode4
peMA.Ode4C13.mode = misc_en_US_carroll.Mode1
peMA.Ode4C13.melody = he.h.m1_en_US_carroll.MegaToMystirion.name
peMA.Ode4C13.incipit = "Lo the riches…"
peMA.Ode4C13.text = "Lo the riches Thomas drew from that unplundered store of treasure -- * yea Your lance-pierced side, Benefactor, that fount of pure theology; * with wisdom and knowledge now * a faithful Didymus fills the entire earth."

peMA.Ode4C14.ode = misc_en_US_carroll.Ode4
peMA.Ode4C14.mode = misc_en_US_carroll.Mode1
peMA.Ode4C14.melody = he.h.m1_en_US_carroll.MegaToMystirion.name
peMA.Ode4C14.incipit = "Your all-blessed tongue…"
peMA.Ode4C14.text = "Your all-blessed tongue, O Thomas, is the subject of our anthem: * for of mortal tongues it was first to proclaim and herald piously * that Jesus is God and Lord * -- for when you touched Him, He filled you with grace divine."

peMA.Ode5C1H.ode = misc_en_US_carroll.Ode5
peMA.Ode5C1H.mode = misc_en_US_carroll.Mode1
peMA.Ode5C1H.melody = he.h.m1_en_US_carroll.EkNyktosOrthrizontesYmnoumenSe.melody
peMA.Ode5C1H.incipit = he.h.m1_en_US_carroll.EkNyktosOrthrizontesYmnoumenSe.incipit
peMA.Ode5C1H.text = he.h.m1_en_US_carroll.EkNyktosOrthrizontesYmnoumenSe.text

peMA.Ode5C11.ode = misc_en_US_carroll.Ode5
peMA.Ode5C11.mode = misc_en_US_carroll.Mode1
peMA.Ode5C11.melody = he.h.m1_en_US_carroll.EkNyktosOrthrizontesYmnoumenSe.name
peMA.Ode5C11.incipit = "Standing in the midst…"
peMA.Ode5C11.text = "Standing in the midst of His till now disheartened friends, * the Savior by His presence there * drives their sad dejection far away: * they exultantly jubilate, hailing the risen Christ."

peMA.Ode5C12.ode = misc_en_US_carroll.Ode5
peMA.Ode5C12.mode = misc_en_US_carroll.Mode1
peMA.Ode5C12.melody = he.h.m1_en_US_carroll.EkNyktosOrthrizontesYmnoumenSe.name
peMA.Ode5C12.incipit = "O how truly laudable…"
peMA.Ode5C12.text = "O how truly laudable, how truly full of dread * the exploit Thomas took in hand: * for he boldly touched with mortal hand, * mid a cloud thick with thunders and lightnings, the side of God."

peMA.Ode5C13.ode = misc_en_US_carroll.Ode5
peMA.Ode5C13.mode = misc_en_US_carroll.Mode1
peMA.Ode5C13.melody = he.h.m1_en_US_carroll.EkNyktosOrthrizontesYmnoumenSe.name
peMA.Ode5C13.incipit = "Unbelief was made…"
peMA.Ode5C13.text = "Unbelief was made for us the mother of belief, * for You, good Christ, arrange all things * by Your wisdom advantageously -- * yea, beneficent Master, for You are the friend of man."

peMA.Ode6C1H.ode = misc_en_US_carroll.Ode6
peMA.Ode6C1H.mode = misc_en_US_carroll.Mode1
peMA.Ode6C1H.melody = he.h.m1_en_US_carroll.TonProfitinDiesosas.melody
peMA.Ode6C1H.incipit = he.h.m1_en_US_carroll.TonProfitinDiesosas.incipit
peMA.Ode6C1H.text = he.h.m1_en_US_carroll.TonProfitinDiesosas.text

peMA.Ode6C11.ode = misc_en_US_carroll.Ode6
peMA.Ode6C11.mode = misc_en_US_carroll.Mode1
peMA.Ode6C11.melody = he.h.m1_en_US_carroll.TonProfitinDiesosas.name
peMA.Ode6C11.incipit = "You abandoned not…"
peMA.Ode6C11.text = "You abandoned not Didymus * as he floundered in doubt's abyss, * but swiftly You reached out with both hands * that he might examine them."

peMA.Ode6C12.ode = misc_en_US_carroll.Ode6
peMA.Ode6C12.mode = misc_en_US_carroll.Mode1
peMA.Ode6C12.melody = he.h.m1_en_US_carroll.TonProfitinDiesosas.name
peMA.Ode6C12.incipit = "Then our Savior…"
peMA.Ode6C12.text = "Then our Savior instructed them, * \"Touch and see for yourselves, my friends, * that I am still clothed in the body: * no, I have not changed at all.\""

peMA.Ode6C13.ode = misc_en_US_carroll.Ode6
peMA.Ode6C13.mode = misc_en_US_carroll.Mode1
peMA.Ode6C13.melody = he.h.m1_en_US_carroll.TonProfitinDiesosas.name
peMA.Ode6C13.incipit = "Thomas handled…"
peMA.Ode6C13.text = "Thomas handled Your side, O Lord, * and, believing, he recognized * his God and his Lord there before him, * when You had come back for him."

